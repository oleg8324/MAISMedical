<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://www.cboss.com/DataTransferTokenizer/1.0" elementFormDefault="qualified" targetNamespace="http://www.cboss.com/DataTransferTokenizer/1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://schemas.microsoft.com/2003/10/Serialization/" />
  <xs:element name="Detokenize">
    <xs:complexType>
      <xs:sequence>
        <xs:element xmlns:q1="http://schemas.microsoft.com/2003/10/Serialization/" minOccurs="0" name="token" type="q1:guid" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="DetokenizeResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="DetokenizeResult" nillable="true" type="xs:string" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Tokenize">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="data" nillable="true" type="xs:string" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TokenizeResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element xmlns:q2="http://schemas.microsoft.com/2003/10/Serialization/" minOccurs="0" name="TokenizeResult" type="q2:guid" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>