      <setting name="MAIS_Web_UserRequestService_AEGIS_WS" serializeAs="String">
        <value>http://aegisservices.secdev.dodd.ohio.gov/AEGIS_WS.svc</value>
      </setting>
      <setting name="MAIS_Web_PublicAccessService_MAIS_DODD_WCF" serializeAs="String">
